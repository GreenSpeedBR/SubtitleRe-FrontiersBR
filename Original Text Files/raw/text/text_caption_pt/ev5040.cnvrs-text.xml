<?xml version="1.0" encoding="utf-8"?>
<cnvrsText>
  <sheet name="pt" index="9">
    <text name="ev5040_010" id="1041641688" font="Range_FNT_subtitle" layout="Range_LAY_subtitle">
      Ele tomou a sua terra natal! Tomou as suas vidas! 
      Você vai deixar que ele faça tudo outra vez? 
      Preciso da sua ajuda! Podemos dar um fim nisso! Por favor!
      
    </text>
    <text name="ev5040_020" id="1041641815" font="Range_FNT_subtitle" layout="Range_LAY_subtitle">
      É agora ou nunca!
    </text>
    <text name="ev5040_030" id="1041641942" font="Range_FNT_subtitle" layout="Range_LAY_subtitle">
      Conte comigo!
    </text>
    <text name="ev5040_040" id="1041642069" font="Range_FNT_subtitle" layout="Range_LAY_subtitle">
      NÃO!
    </text>
    <text name="ev5040_050" id="1041642196" font="Range_FNT_subtitle" layout="Range_LAY_subtitle">
      Por favor... cuide... do meu pai...
    </text>
  </sheet>
